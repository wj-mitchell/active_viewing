	file = {Hasson et al. - 2004 - Intersubject Synchronization of Cortical Activity .pdf:C\:\\Users\\wjpmi\\Zotero\\storage\\6YDZKNQK\\Hasson et al. - 2004 - Intersubject Synchronization of Cortical Activity .pdf:application/pdf},
