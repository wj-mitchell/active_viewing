@article{torre_putting_2018,
	title = {Putting {Feelings} {Into} {Words}: {Affect} {Labeling} as {Implicit} {Emotion} {Regulation}},
	volume = {10},
	issn = {1754-0739, 1754-0747},
	shorttitle = {Putting {Feelings} {Into} {Words}},
	url = {http://journals.sagepub.com/doi/10.1177/1754073917742706},
	doi = {10.1177/1754073917742706},
	abstract = {Putting feelings into words, or “affect labeling,” can attenuate our emotional experiences. However, unlike explicit emotion regulation techniques, affect labeling may not even feel like a regulatory process as it occurs. Nevertheless, research investigating affect labeling has found it produces a pattern of effects like those seen during explicit emotion regulation, suggesting affect labeling is a form of implicit emotion regulation. In this review, we will outline research on affect labeling, comparing it to reappraisal, a form of explicit emotion regulation, along four major domains of effects—experiential, autonomic, neural, and behavioral—that establish it as a form of implicit emotion regulation. This review will then speculate on possible mechanisms driving affect labeling effects and other remaining unanswered questions.},
	language = {en},
	number = {2},
	urldate = {2021-08-09},
	journal = {Emotion Review},
	author = {Torre, Jared B. and Lieberman, Matthew D.},
	month = apr,
	year = {2018},
	keywords = {Distraction, Self-Regulation, Reappraisal, Emotion, Emotion Regulation, Implicit Regulation, Affect Labeling, Review},
	pages = {116--124},
	file = {Torre and Lieberman - 2018 - Putting Feelings Into Words Affect Labeling as Im.pdf:C\:\\Users\\wjpmi\\Zotero\\storage\\YCYIIKNK\\Torre and Lieberman - 2018 - Putting Feelings Into Words Affect Labeling as Im.pdf:application/pdf;Torre and Lieberman - 2018 - Putting Feelings Into Words Affect Labeling as Im.pdf:C\:\\Users\\wjpmi\\Zotero\\storage\\GN2UACZV\\Torre and Lieberman - 2018 - Putting Feelings Into Words Affect Labeling as Im.pdf:application/pdf},
